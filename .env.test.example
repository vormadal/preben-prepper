# Test Database Configuration
# Copy this file to .env.test and update the values for your test environment

# Test Database URL - Use a separate database for testing
DATABASE_URL="postgresql://test_user:test_password@localhost:5432/preben_prepper_test"

# JWT Secret for testing
JWT_SECRET="test-jwt-secret-key-for-testing-only"

# Test environment
NODE_ENV="test"

# Disable rate limiting in tests
DISABLE_RATE_LIMITING="true"

# Test port (optional, tests don't start a server)
PORT=3001
